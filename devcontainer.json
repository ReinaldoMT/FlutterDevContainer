{
    "name": "Flutter Dev Container",
    "image": "flutter-dev-container:latest",
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.flutter",
                "dart-code.dart-code"
            ],
            "settings": {
                "dart.sdkPath": "/usr/local/flutter/bin/cache/dart-sdk"
            }
        }
    }
}
